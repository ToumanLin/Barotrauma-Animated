<?xml version="1.0" encoding="utf-8"?>
<Override>
  <Items>
    <Item identifier="healthscanner">
      <inherit identifier="healthscanner">
        <doinherit />
        <replace sel="./@tags">
          smallitem,clothing,Wf_helmet_module
        </replace>
        <replace sel="./Sprite/@texture">
          %ModDir%/Content/Items/Jobgear/headgears.png
        </replace>
        <replace sel="./Sprite/@sourcerect">
          110,355,72,35
        </replace>
        <replace sel="./Wearable[@limbtype='Head']/sprite[@limb='Head']/@texture">
          %ModDir%/Content/Items/Jobgear/headgears.png
        </replace>
        <replace sel="./Wearable[@limbtype='Head']/sprite[@limb='Head']/@sourcerect">
          110,355,72,27
        </replace>
        <replace sel="./Wearable[@limbtype='Head']/sprite[@limb='Head']/@origin">
          0.642,0.744
        </replace>
      </inherit>
    </Item>
    <Item identifier="doctorsuniform1">
      <inherit identifier="doctorsuniform1">
        <doinherit />
        <replace sel="./Wearable/sprite/@texture">
          Medic1.png
        </replace>
        <replace sel="./Wearable/sprite/@hidelimb">
          false
        </replace>
        <replace sel="./Wearable/sprite[@limb='RightForearm' or @limb='LeftForearm' or @limb='Waist' or @limb='LeftFoot' or @limb='RightFoot']/@hidelimb">
          true
        </replace>
      </inherit>
    </Item>
    <Item identifier="doctorsuniform2">
      <inherit identifier="doctorsuniform2">
        <doinherit />
        <replace sel="./Wearable/sprite/@texture">
          Medic2.png
        </replace>
        <replace sel="./Wearable/sprite/@hidelimb">
          false
        </replace>
        <replace sel="./Wearable/sprite[@limb='RightForearm' or @limb='LeftForearm' or @limb='Waist' or @limb='LeftFoot' or @limb='RightFoot']/@hidelimb">
          true
        </replace>
      </inherit>
    </Item>
  </Items>
</Override>